{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module",
        "ecmaVersion": 2016
    },
    "plugins": [
        "react",
        "babel"
    ],
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "quotes": "off",
        "semi": "off",
        "react/prop-types": "off",
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-console": "off",
        "no-empty-pattern": "warn",
        "react/display-name": "off",
        "no-empty": "warn",
        "react/jsx-pascal-case": "off"
    }
}
